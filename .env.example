PORT=8091
BACKEND_BASE_URL=http://localhost:8091

# Optional frontend callback URLs
# Success URL receives: token, provider, expiresAt
FRONTEND_GITHUB_SUCCESS_URL=
FRONTEND_GITHUB_ERROR_URL=

# GitHub OAuth app credentials
GITHUB_CLIENT_ID=
GITHUB_CLIENT_SECRET=
GITHUB_CALLBACK_URL=

# Optional access control lists (comma separated GitHub usernames, lowercase recommended)
# If GITHUB_ALLOWED_USERS is empty, any GitHub account can sign in.
GITHUB_ALLOWED_USERS=
# These users get admin role with '*' permissions.
GITHUB_ADMIN_USERS=

# Session
SESSION_TTL_MS=86400000

